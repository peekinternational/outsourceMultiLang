              SweetAlert.swal('No Money!', 'Not enough amount in wallet for upgradation of project.', 'error');
            });
          }
          else{
            SweetAlert.close();
          }
        });
      }else{
        var project = $scope.project;
        project.$update(function (response) {
          $scope.project = response;
        }, function (fail) {
          toastr.error('Edit error', 'Oops, something went wrong');
