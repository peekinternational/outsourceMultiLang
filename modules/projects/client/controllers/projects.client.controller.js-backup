      } else if (pakage_name === 'urgent') {
        if (value) {
          $scope.totalAmount = $scope.totalAmount + ($scope.pakagePrices.urgent*$rootScope.latestCurrencyRate[$scope.project.currency.code]);
        } else {
          $scope.totalAmount = $scope.totalAmount - ($scope.pakagePrices.urgent*$rootScope.latestCurrencyRate[$scope.project.currency.code]);
